{
  "name": "ght-bar-api",
  "version": "0.0.1",
  "description": "API for GHT Bar",
  "author": "Amsterdam-3000",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "generate": "prisma generate",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules npx jest --watch",
    "test:cov": "NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
    "test:debug": "node --experimental-vm-modules --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules npx jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "4.7.4",
    "@apollo/server-plugin-response-cache": "^4.1.2",
    "@nestjs/apollo": "12.0.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^12.0.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/throttler": "^4.1.0",
    "@prisma/client": "^4.16.1",
    "apollo-server-plugin-base": "^3.7.2",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express-useragent": "^1.0.15",
    "file-type": "^18.5.0",
    "graphql": "^16.7.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-query-complexity": "^0.12.0",
    "graphql-upload": "^15.0.2",
    "joi": "^17.9.2",
    "minio": "^7.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "type-graphql": "^2.0.0-beta.2",
    "typegraphql-nestjs": "^0.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.3",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.2",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "jest-extended": "^4.0.0",
    "jest-mock-extended": "^3.0.4",
    "prettier": "^2.3.2",
    "prisma": "^4.16.1",
    "prismock": "^1.14.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typegraphql-prisma": "^0.26.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageProvider": "v8",
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "../test/testExtend.ts"
    ]
  }
}
